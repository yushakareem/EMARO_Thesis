<?xml version="1.0"?>
<rdf:RDF xmlns="http://www.semanticweb.org/emaroLab/YushaKareem/PlacingOntology#"
     xml:base="http://www.semanticweb.org/emaroLab/YushaKareem/PlacingOntology"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:swrlb="http://www.w3.org/2003/11/swrlb#"
     xmlns:swrl="http://www.w3.org/2003/11/swrl#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:PlacingOntology="http://www.semanticweb.org/emaroLab/YushaKareem/PlacingOntology#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:swrla="http://swrl.stanford.edu/ontologies/3.3/swrla.owl#">
    <owl:Ontology rdf:about="http://www.semanticweb.org/emaroLab/YushaKareem/PlacingOntology">
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Naming Convention:

In Individuals:
A: stands for Ambient
S: stands for Sensor
F: stands for Furniture
R: stands for Room
E: stands for Elicon
W: stands for Watch
V: stands for virtual</rdfs:comment>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled -->

    <owl:AnnotationProperty rdf:about="http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.semanticweb.org/emaroLab/YushaKareem/PlacingOntology#hasBeenDetectedNear -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/emaroLab/YushaKareem/PlacingOntology#hasBeenDetectedNear">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/emaroLab/YushaKareem/PlacingOntology#Person"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/emaroLab/YushaKareem/PlacingOntology#Furniture"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Person hasBeenDetectedNear Furniture
(Note: No postural change, sitting or lying or something)</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/emaroLab/YushaKareem/PlacingOntology#hasFurniture -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/emaroLab/YushaKareem/PlacingOntology#hasFurniture">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/emaroLab/YushaKareem/PlacingOntology#isFurnitureOf"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/emaroLab/YushaKareem/PlacingOntology#Room"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/emaroLab/YushaKareem/PlacingOntology#Furniture"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Room &apos;hasFurniture&apos; Furniture</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/emaroLab/YushaKareem/PlacingOntology#hasFurnitureTheSensor -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/emaroLab/YushaKareem/PlacingOntology#hasFurnitureTheSensor">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/emaroLab/YushaKareem/PlacingOntology#isSensorWithTheFurniture"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/emaroLab/YushaKareem/PlacingOntology#Furniture"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/emaroLab/YushaKareem/PlacingOntology#Sensor"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/emaroLab/YushaKareem/PlacingOntology#hasRoom -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/emaroLab/YushaKareem/PlacingOntology#hasRoom">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/emaroLab/YushaKareem/PlacingOntology#isRoomOf"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/emaroLab/YushaKareem/PlacingOntology#Ambient"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/emaroLab/YushaKareem/PlacingOntology#Room"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">An Ambient instance hasRoom some instance of Room</rdfs:comment>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The Ambient(House) has some Room</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/emaroLab/YushaKareem/PlacingOntology#hasRoomTheSensor -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/emaroLab/YushaKareem/PlacingOntology#hasRoomTheSensor">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/emaroLab/YushaKareem/PlacingOntology#isSensorInRoom"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/emaroLab/YushaKareem/PlacingOntology#Room"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/emaroLab/YushaKareem/PlacingOntology#Sensor"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Room hasRoomTheSensor Sensor</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/emaroLab/YushaKareem/PlacingOntology#isBeingIn -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/emaroLab/YushaKareem/PlacingOntology#isBeingIn">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/emaroLab/YushaKareem/PlacingOntology#Person"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/emaroLab/YushaKareem/PlacingOntology#Room"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">An instance of Person isBeingIn an instance of a Room</rdfs:comment>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Can be derived from various sensors (PIR, Elico system)</rdfs:comment>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">User isBeingIn some Room/Area</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/emaroLab/YushaKareem/PlacingOntology#isDoingGesture -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/emaroLab/YushaKareem/PlacingOntology#isDoingGesture">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/emaroLab/YushaKareem/PlacingOntology#Person"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/emaroLab/YushaKareem/PlacingOntology#GestureSense"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Person isDoingGesture Gesture</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/emaroLab/YushaKareem/PlacingOntology#isFurnitureOf -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/emaroLab/YushaKareem/PlacingOntology#isFurnitureOf">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Some Furniture isFurnitureOf some Room</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/emaroLab/YushaKareem/PlacingOntology#isOccupying -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/emaroLab/YushaKareem/PlacingOntology#isOccupying">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/emaroLab/YushaKareem/PlacingOntology#Person"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/emaroLab/YushaKareem/PlacingOntology#Furniture"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">An instance of a Person isOccupying some instance of Furniture</rdfs:comment>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">User Occupying a Furniture with sitting(Postural Change)</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/emaroLab/YushaKareem/PlacingOntology#isRoomOf -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/emaroLab/YushaKareem/PlacingOntology#isRoomOf">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Ambient is Located in Ambient</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/emaroLab/YushaKareem/PlacingOntology#isSensorInRoom -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/emaroLab/YushaKareem/PlacingOntology#isSensorInRoom">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/emaroLab/YushaKareem/PlacingOntology#isSensorWithTheFurniture -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/emaroLab/YushaKareem/PlacingOntology#isSensorWithTheFurniture"/>
    


    <!-- http://www.semanticweb.org/emaroLab/YushaKareem/PlacingOntology#isSensorWithThePerson -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/emaroLab/YushaKareem/PlacingOntology#isSensorWithThePerson">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/emaroLab/YushaKareem/PlacingOntology#isWithSensor"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Instance_of_Elicon/Watch isSensorWithThePerson Instance_Of_Person</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/emaroLab/YushaKareem/PlacingOntology#isSpaceNearTo -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/emaroLab/YushaKareem/PlacingOntology#isSpaceNearTo">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#SymmetricProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/emaroLab/YushaKareem/PlacingOntology#Room"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/emaroLab/YushaKareem/PlacingOntology#Room"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Area/Room isNearTo Area/Room</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/emaroLab/YushaKareem/PlacingOntology#isWithSensor -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/emaroLab/YushaKareem/PlacingOntology#isWithSensor">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/emaroLab/YushaKareem/PlacingOntology#Person"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/emaroLab/YushaKareem/PlacingOntology#Sensor"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Instance_of_person isWithSensor instance_of_Elicon/Watch</rdfs:comment>
    </owl:ObjectProperty>
    


