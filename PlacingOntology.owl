    <!-- http://www.semanticweb.org/emaroLab/YushaKareem/PlacingOntology#hasTypeTimeStamp -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/emaroLab/YushaKareem/PlacingOntology#hasTypeTimeStamp">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:domain>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/emaroLab/YushaKareem/PlacingOntology#hasTypeTimeStamp"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onDataRange rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
            </owl:Restriction>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>
    


