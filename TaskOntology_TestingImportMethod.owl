                                                                        <swrl:argument1 rdf:resource="http://www.semanticweb.org/emaroLab/YushaKareem/TaskOntology#St_Ts_OccupyingFurniture"/>
                                                                        <swrl:argument2 rdf:resource="http://www.semanticweb.org/emaroLab/YushaKareem/PlacingOntology#S_W_V_Standing"/>
                                                                    </rdf:Description>
                                                                </rdf:first>
